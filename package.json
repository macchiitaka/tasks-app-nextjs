{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --fix",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:stylelint": "stylelint \"**/*.{jsx,tsx}\" --cache",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "start": "next start"
  },
  "dependencies": {
    "@socialgouv/matomo-next": "^1.2.2",
    "axios": "0.21.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-query": "3.17.2",
    "styled-components": "5.3.0",
    "styled-normalize": "8.0.7"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^11.0.1",
    "@takamachi/eslint-config": "^1.0.5",
    "@takamachi/prettier-config": "^1.1.0",
    "@types/babel__core": "^7.1.14",
    "@types/eslint": "7.2.13",
    "@types/is-ci": "3.0.0",
    "@types/prettier": "^2.3.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-is": "17.0.1",
    "@types/styled-components": "5.1.10",
    "@types/stylelint": "13.13.0",
    "eslint": "7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-jest-dom": "3.9.0",
    "eslint-plugin-testing-library": "4.6.0",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "pathpida": "0.15.2",
    "pinst": "2.1.6",
    "prettier": "2.3.1",
    "sort-package-json": "1.50.0",
    "stylelint": "13.13.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-property-sort-order-smacss": "7.1.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "5.0.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.3.4",
    "utility-types": "3.10.0"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "volta": {
    "node": "14.17.1",
    "npm": "7.1.0"
  }
}
