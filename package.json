{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --fix",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:stylelint": "stylelint \"**/*.{jsx,tsx}\" --cache",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s storybook-public",
    "build-storybook": "build-storybook -s storybook-public"
  },
  "dependencies": {
    "axios": "0.22.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-query": "3.25.1",
    "styled-components": "5.3.1",
    "styled-normalize": "8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@next/eslint-plugin-next": "^11.1.2",
    "@storybook/addon-a11y": "^6.3.10",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@takamachi/eslint-config": "^1.1.1",
    "@takamachi/prettier-config": "^1.1.0",
    "@types/babel__core": "^7.1.16",
    "@types/eslint": "7.28.1",
    "@types/is-ci": "3.0.0",
    "@types/prettier": "^2.4.1",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "@types/react-is": "17.0.2",
    "@types/styled-components": "5.1.15",
    "@types/stylelint": "13.13.2",
    "babel-loader": "^8.2.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-jest": "24.5.2",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-testing-library": "4.12.4",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "lint-staged": "11.2.1",
    "msw": "^0.35.0",
    "msw-storybook-addon": "^1.4.0",
    "npm-run-all": "4.1.5",
    "pathpida": "0.17.0",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "sort-package-json": "1.52.0",
    "stylelint": "13.13.1",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-property-sort-order-smacss": "7.1.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "5.0.2",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.4.3",
    "utility-types": "3.10.0"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "msw": {
    "workerDirectory": "storybook-public"
  },
  "volta": {
    "node": "14.18.0",
    "npm": "8.0.0"
  }
}
