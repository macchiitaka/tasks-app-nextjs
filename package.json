{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-p fmt:**",
    "fmt:eslint": "eslint '**/*.{js,jsx,ts,tsx}' --fix --cache",
    "fmt:json": "prettier --write '**/*.json'",
    "fmt:md": "prettier --write '**/*.md'",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint '**/*.{js,jsx,ts,tsx}' --cache",
    "lint:stylelint": "stylelint '**/**.{jsx,tsx}' --cache",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "start": "next start"
  },
  "dependencies": {
    "axios": "0.21.1",
    "next": "10.0.7",
    "preact": "10.5.12",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "react-query": "3.9.6",
    "styled-components": "5.2.1",
    "styled-normalize": "8.0.7"
  },
  "devDependencies": {
    "@types/eslint": "7.2.6",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/prettier": "2.2.0",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-is": "17.0.0",
    "@types/styled-components": "5.1.7",
    "@types/stylelint": "9.10.1",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "@typescript-eslint/typescript-estree": "4.15.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "3.10.1",
    "eslint-plugin-unicorn": "28.0.1",
    "husky": "5.0.9",
    "is-ci": "2.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "pathpida": "0.13.0",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "sort-package-json": "1.48.1",
    "stylelint": "13.10.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-property-sort-order-smacss": "7.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.1.5",
    "utility-types": "3.10.0"
  },
  "engines": {
    "node": "12.x",
    "npm": "7.x",
    "yarn": "1.x"
  }
}
