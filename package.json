{
  "name": "tasks-app-nextjs",
  "version": "1.0.0",
  "repository": "git@github.com:macchiitaka/tasks-app-nextjs.git",
  "license": "MIT",
  "author": "macchiitaka <zxcvbnmtacashi@gmail.com>",
  "scripts": {
    "build": "pathpida --enableStatic && next build",
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --enableStatic --watch",
    "fmt": "run-s fmt:prettier fmt:package-json fmt:eslint",
    "fmt:prettier": "prettier --write --tab-width 2 \"**/*.{js,jsx,ts,tsx,json,yml,yaml,md}\"",
    "fmt:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content --fix",
    "fmt:package-json": "sort-package-json",
    "postinstall": "run-p postinstall:**",
    "lint": "run-p lint:**",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\" --cache --cache-strategy content",
    "lint:stylelint": "stylelint \"**/*.{jsx,tsx}\" --cache",
    "lint:type-check": "tsc -p . --noEmit",
    "postinstall:husky": "is-ci || husky install;",
    "postinstall:pathpida": "pathpida --enableStatic",
    "start": "next start"
  },
  "dependencies": {
    "@socialgouv/matomo-next": "^1.2.1",
    "axios": "0.21.1",
    "next": "10.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-query": "3.13.0",
    "styled-components": "5.2.1",
    "styled-normalize": "8.0.7"
  },
  "devDependencies": {
    "@takamachi/eslint-config": "^0.1.1",
    "@takamachi/prettier-config": "^1.0.1",
    "@types/eslint": "7.2.7",
    "@types/is-ci": "3.0.0",
    "@types/prettier": "^2.2.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-is": "17.0.0",
    "@types/styled-components": "5.1.9",
    "@types/stylelint": "9.10.1",
    "eslint": "7.23.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jest-dom": "3.7.0",
    "eslint-plugin-testing-library": "3.10.2",
    "husky": "6.0.0",
    "is-ci": "3.0.0",
    "lint-staged": "10.5.4",
    "npm-run-all": "4.1.5",
    "pathpida": "0.14.0",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "sort-package-json": "1.49.0",
    "stylelint": "13.12.0",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-property-sort-order-smacss": "7.0.0",
    "stylelint-config-standard": "21.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.2.3",
    "utility-types": "3.10.0"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x",
    "yarn": "1.x"
  },
  "volta": {
    "node": "14.16.0",
    "yarn": "1.22.10"
  }
}
